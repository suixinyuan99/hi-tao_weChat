.title{
    margin-top: 10rpx;
}
.title input{
    background: #fff;
    width: 720rpx;
    font-size: 30rpx;
    height: 80rpx;
    line-height: 80rpx;
    padding-left: 30rpx;
    border-bottom: 10rpx solid #f5f5f5;
}
.introduce textarea{
    background: #fff;
    width: 720rpx;
    font-size: 30rpx;
    height: 180rpx;
    line-height: 70rpx;
    padding-left: 30rpx;
    padding-top: 30rpx;
}
.img{
    background: #fff;
}
.img image{
    display: block;
    width: 120rpx;
    height: 120rpx;
    border: 1px solid #cccccc;
    margin: 0 0 0 40rpx;
}
.img text{
    font-size: 25rpx;
    padding: 40rpx;
    color: #ddd;
}



.money,.weixin,.telephone,.contact,.contact2,.contact3{
    margin:40rpx 40rpx 0 40rpx;
    background: #fff;
}
.money{
    border-bottom: 1px solid #ccc;
    position: relative;
}
.weixin input,.telephone input,.contact input,.contact2 input,.contact3 input{
    display: inline-block;
    font-size: 30rpx;
    height: 40rpx;
    line-height: 40rpx;
    padding-left: 15rpx;
    position: absolute;
    top: 0rpx;
    left:0rpx;
}
.money input{
    display: inline-block;
    font-size: 30rpx;
    height: 40rpx;
    line-height: 40rpx;
    position: absolute;
    top: 0rpx;
    left:155rpx;
}
.add{
    width: 40rpx;
    height: 40rpx;
    padding: 5rpx;
    position: absolute;
    top:0rpx;
    right: 0rpx;
    border-radius: 50%;
    border: 1px solid #ccc;
}
.contact input,.contact2 input,.contact3 input{
    margin-left: 200rpx;
}
.picker{
    background: #fff;
    display: flex;
    width: 100%;
    margin: 20rpx 40rpx 0 40rpx;
}
.type,.area{
    border-bottom: 1px solid #ccc;
    width: 42%;
}
.type{
    margin: 0 20px;
}
.label{
    display: inline-block;
    width: 20%rpx;
    text-align: center;
    font-size: 30rpx;
    height: 40rpx;
    line-height: 40rpx;
    margin: 40rpx 0 0 0;
}
.data{
    border-radius: 10rpx;
    display: inline-block;
    width: 31%;
    font-size: 30rpx;
    height: 50rpx;
    line-height: 50rpx;
    position: relative;
}
.productType{
    width: 39%;
    padding-left: 20rpx;
}
.down{
    position: absolute;
    right: 5rpx;
    top: 10rpx;
    width: 30rpx;
    height: 30rpx;
}
.Label{
    display: inline-block;
    width: 120rpx;
    text-align: center;
    font-size: 30rpx;
    height: 40rpx;
    line-height: 40rpx;
}
.read{
    background: #fff;
    margin: 40rpx;
    font-size: 30rpx;
}
.read checkbox-group{
    display: inline-block;
}
.read text{
    color: #96B8EC;
}
button{
    background: #F3490A;
    width: 90%;
    text-align: center;
    height: 100rpx;
    line-height: 100rpx;
}
.input{
    display: inline-block;
    color: #666;
    border: 1px solid #ccc;
    margin-left: 50rpx;
    width: 60%;
    font-size: 30rpx;
    height: 40rpx;
    padding-left: 15rpx;
}
.contact,.contact2,.contact3,.weixin,.telephone{
    position: relative;
}
.contact,.contact2,.contact3{
    display: flex;
}
.contact picker,.contact2 picker,.contact3 picker{
    width: 450rpx;
}
.showImage{
    display: flex;
    flex-wrap: wrap;
}
.showImage image{
    margin-left: 40rpx;
    margin-top: 40rpx;
    width: 120rpx;
    height: 120rpx;
}
.iconpic{
    position: relative;
}
.iconfont{
    position: absolute;
    top: 10rpx;
    left: -15rpx;
}
.money{
    position: relative;
}
.money input{
    padding-left: 30rpx;
}
.moneyStyle{
    position: absolute;
    top: 4rpx;
    left: 144rpx;
    color: aaa;
}
.pickerView{
    height: 56rpx;
    line-height: 56rpx;
    font-size: 30rpx;
    text-align: center;
}
.phcolor{
    color: #ddd;
}
.white{
    color: white;
}
.icon-xinjian{
    font-size: 40px;
    text-align: center;
}
