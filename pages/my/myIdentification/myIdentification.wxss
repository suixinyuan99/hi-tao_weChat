.container{
	margin: 100rpx auto;
}
.username{
	text-align: center;
	color: #E34A07;
}
.headImage{
	width: 100rpx;
	height: 100rpx;
	margin: 0 auto;
	border-radius: 50%;
}
.container .img{
	width: 60%;
	height: 300rpx;
	border: 1px solid #999;
	position: relative;
	margin-top: 30rpx;
	background: #E34A07;
	text-align: center;
	line-height: 300rpx;
}
.iconfont {
	font-size: 80px;
	color: white;
}
.img image{
	width: 100%;
	height: 100%;
	border-radius: 20rpx;
}
