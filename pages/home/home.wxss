/* 搜索栏 */

.searchBarBox {
  position: fixed;
  background: #FF6201;
  display: flex;
  height: 120rpx;
  width: 100%;
  top: 0rpx;
  align-items: center;
  justify-content: center;
  z-index: 100;
}

.search_arr {
  background: #fff;
  width: 624rpx;
  height: 80rpx;
  border: 1px solid #d0d0d0;
  border-radius: 30rpx;
  display: flex;
  align-items: center;
  box-shadow: 0rpx 4rpx 12rpx 0rpx rgba(0, 0, 0, 0.4);
}

.onSearch_arr {
  background: #fff;
  width: 570rpx;
  height: 80rpx;
  border: 1px solid #d0d0d0;
  border-radius: 30rpx;
  display: flex;
  align-items: center;
  box-shadow: 0rpx 4rpx 12rpx 0rpx rgba(0, 0, 0, 0.4);
}

.searchIcon {
  padding-left: 20rpx;
  z-index: 2;
  text-align: center;

}

.searchText {
  padding-left: 40rpx;
  color: #101010;
  font-family: Arial;
  font-size: 36rpx;
}

.iconMessage {
  color: #fff;
  font-size: 48rpx;
  margin-left: 14rpx;
}


/* 取消按钮 */
.cancelBtn{
  position: relative;
  top:0;
  right:0;
}
.cancelBtn button{
  vertical-align: middle;
  background-color: transparent;
  color:#fff;
  letter-spacing: 8rpx;
  font-size: 30rpx;
}
.cancelBtn button::after{
  border:none;
}

/* 选择框 */
.data{
  border-radius: 10rpx;
  display: inline-block;
  margin-left: 40rpx;
  margin-right: 20rpx;
  width: 100rpx;
  font-size: 30rpx;
  height: 50rpx;
  line-height: 50rpx;
  position: relative;
}
.down{
  position: absolute;
  right: 5rpx;
  top: 10rpx;
  width: 30rpx;
  height: 30rpx;
}

/* 搜索提示 */
.searchBarShade{
  background-color: #fff;
  width: 100%;
  position: fixed;
  z-index: 20;
  display: flex;
  flex-wrap:wrap;
  justify-content:space-around;
  align-items: flex-start;
  align-content: flex-start;
}

.history{
  display: flex;
  width: 100%;
  margin-top: 30rpx;
  margin-left: 30rpx;
  margin-bottom: 50rpx;
  flex-wrap:wrap;
  justify-content:space-around;
  align-items: flex-start;
  align-content: flex-start;
}
.historyTopic{
  width: 100%;
  height: 48rpx;
  color: rgba(16, 16, 16, 1);
  font-size: 32rpx;
  text-align: left;
  font-family: PingFangSC-regular;
}

.historyItem{
margin-top: 40rpx;
width: 40%;
height: 42rpx;
color: rgba(111, 111, 111, 1);
font-size: 28rpx;
text-align: left;
font-family: PingFangSC-regular;
}


/* 轮播图 */
.indexSwiper {
  height: 350rpx;
  margin-top: 120rpx;
}

.indexSwiperItem {
  width: 100%;
  height: 100%;
}

.indexSwiperImage {
  width: 100%;
  height: 100%;
}


/* 导航栏 */
.swiper-tab {
  display: flex;
  align-items: flex-end;
  justify-content: center;
  z-index: 10;
  position: sticky;
  top: 120rpx; 
  width: 100%;
  border-bottom: 2rpx solid #fff;
  height: 88rpx;
  text-align: center;
  background: #fff;

}

.swiper-tab-list {
  font-size: 28rpx;
  display: inline-block;
  width: 20%;
  color: #000;
  margin-bottom: 4rpx;
  line-height: 70rpx;
  font-family: Arial;
}

.swiper-tab-list view {
  width: 100%;
  text-align: center;
  margin: auto;
  border-bottom: 4rpx solid #ccc;
}

.swiper-tab-list .on {
  color: #F3490A;
  border-bottom: 4rpx solid #F3490A;
}



/* 显示区 */


.item{
  width: 100%;
}

.swiper-box {
  display: flex;
  height: auto;
  width: 100%;
  overflow: hidden;
  background-color: rgba(181, 175, 175, 0.11);
}




.info-item {
  display: -webkit-flex;
  display: -moz-flex;
  display: -ms-flex;
  display: -o-flex;
  display: flex;
  flex-direction: row;
  position: relative;
  /*padding-bottom: 30rpx;
  padding-right:20rpx;*/
  padding: 20rpx 20rpx 20rpx 30rpx;
  background-color: #fff;
  flex-wrap:wrap;
}

.speItem {
  display: -webkit-flex;
  display: -moz-flex;
  display: -ms-flex;
  display: -o-flex;
  display: flex;
  flex-direction: row;
  position: relative;
  /*padding-bottom: 30rpx;
  padding-right:20rpx;*/
  padding: 20rpx 20rpx 10rpx 40rpx;
  margin:10rpx 5rpx 10rpx 20rpx;
  background-color: #fff;
  border-radius: 20rpx 20rpx 20rpx 20rpx;
  flex-wrap:wrap;
}



.info-item .mask {
  position: absolute;
  height: 220rpx;
  line-height: 280rpx;
  width: 100%;
  text-align: center;
  vertical-align: middle;
  z-index: 990;
  background-color: rgba(255, 255, 255, 0.7);
}


.info-item .mask image {
  /* transform: rotate(-15deg); */
  position: absolute;
  bottom: 0;
  height: 120rpx;
  width: 120rpx;
  right: 60rpx;
  
}
.allItem .mask {
  position: absolute;
  height: 100%;
  line-height: 500rpx;
  width: 100%;
  text-align: center;
  vertical-align: middle;
  z-index: 990;
  background-color: rgba(255, 255, 255, 0.7);
}


.allItem .mask image {
  height: 120rpx;
  width: 120rpx;
  position: absolute;
  bottom: 90rpx;
  right: 70rpx;  
}



